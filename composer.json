{
    "name": "zendframework/zend-auradi-config",
    "description": "PSR-11 Aura.DI container configurator for ZF applications",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf",
        "zendframework",
        "aura di",
        "container",
        "psr-11"
    ],
    "support": {
        "issues": "https://github.com/zendframework/zend-auradi-config/issues",
        "source": "https://github.com/zendframework/zend-auradi-config",
        "slack": "https://zendframework-slack.herokuapp.com",
        "forum": "https://discourse.zendframework.com/c/questions"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "aura/di": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.22 || ^6.3.1",
        "zendframework/zend-coding-standard": "~1.0.0"
    },
    "conflict": {
        "container-interop/container-interop": "<1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Zend\\AuraDi\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\AuraDi\\Config\\": "test"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    }
}
